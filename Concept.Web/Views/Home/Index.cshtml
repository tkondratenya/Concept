@model IEnumerable<Concept.Web.Models.ResourceViewModel>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p>@Html.ActionLink("Create Resource", "CreateResource", "Home")</p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Resources</h2>
        @foreach (var res in Model)
        {
            <p>@res.Name</p>
        }
    </div>
</div>